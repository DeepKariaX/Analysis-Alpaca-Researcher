# Analysis Alpaca Research Configuration Example
# Copy this file to .env and modify values as needed

# Search Configuration
CDR_MAX_RESULTS=3
CDR_DEFAULT_NUM_RESULTS=2
CDR_WEB_TIMEOUT=10.0
CDR_ACADEMIC_TIMEOUT=10.0
CDR_EXTRACTION_TIMEOUT=8.0
CDR_USER_AGENT="Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"

# Content Configuration
CDR_MAX_CONTENT_SIZE=8000
CDR_MAX_EXTRACTION_SIZE=100000
CDR_MAX_PARAGRAPHS=5
CDR_MAX_ELEMENTS=8
CDR_MAX_SNIPPET_LENGTH=200

# Server Configuration
CDR_LOG_LEVEL=INFO
CDR_LOG_FILE=
CDR_AUTO_INSTALL_DEPS=true